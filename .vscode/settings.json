{
	"workbench.colorTheme": "Monokai Vibrant (operator)",
	"editor.fontFamily": "'Roboto Mono', 'SF Mono', 'Fira Code', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.tabSize": 2,
	"editor.formatOnSave": true,
	"editor.fontSize": 13,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.detectIndentation": false,
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	}
}
